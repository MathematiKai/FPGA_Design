

/***************************** Include Files *******************************/
#include "PWM_ctrl.h"
#include "sleep.h"

/************************** Function Definitions ***************************/
void PWM(UINTPTR baseaddr , int sw)
{
	u32 PWM ;
	PWM_CTRL_mWriteReg(baseaddr , 0 , sw);
	sleep(5);

	PWM_CTRL_mWriteReg(baseaddr , 0 , 0x00);

}

void PWM_reset(UINTPTR baseaddr){
	PWM_CTRL_mWriteReg(baseaddr , 0 , 4);
	PWM_CTRL_mWriteReg(baseaddr , 0 , 0);
}
